package org.integrallis.drools
 
rule "Matches"
	when
		person1 : Person()
		person2 : Person(
		    //this != person1,
		    this > person1 // or 
		    //person2.compareTo(person1) > 0
		)
		person3: Person(
			this > person2
		)
	then
		System.out.println( "{" + person1 + ", " + person2 + ", " + person3  + "}" ); 
end